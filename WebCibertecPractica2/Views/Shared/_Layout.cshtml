                            </ul>

                            </li>
                        </ul>
                </nav>               

                </div>
        </div>

    </header>
    <section class="container body-content">
        @RenderBody()
    </section>
   
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
